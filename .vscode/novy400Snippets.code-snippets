		"scope": "rpgle",
		"body": [
			"dcl-proc $1 export;",
			"\tdcl-pi *n ;",
			"\tend-pi;",
			"monitor;",
			"// initialisation",
			"// traitement",
			"// finalisation ",
			"  $0",
			"on-error;",
			"// ðŸ’¥hoorreur ! malheur ! YABUG !",
			"snd-msg *escape %msg('Horreur ! dans ' + %trim(%proc());" 
			"endmon;"
			"end-proc;",
			"$4"
		],
		"description": "Create prototype for external program"
	}
